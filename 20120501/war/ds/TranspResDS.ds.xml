<DataSource
	ID="TranspResDS"
	serverType="sql"
	tableName="transp_resource"
	qualifyColumnNames="false"
	dropExtraFields = "false">
	<fields>
		<field name="transp_res_id"					type="integer"		title="transp_res_id"		hidden="true" primaryKey="true" />
		<field name="transp_type_id"				type="integer"		title="transp_type_id" 		hidden="true"/>
		<field name="name_descr"					type="text"			title="name_descr"/>				
		<field name="loggedUserName"				type="text"			title="Logged UserName" 	hidden="true" />
	</fields>

	<operationBindings>		
    	<operationBinding operationId="searchAllTranspPlanesForCombos" operationType="fetch">
    		<selectClause>   			
    			t.transp_res_id,
    			t.transp_type_id,
    			t.name_descr 
			</selectClause> 
			<tableClause>transp_resource t</tableClause>	
			<whereClause>
				($defaultWhereClause)
			</whereClause>			
    	</operationBinding>
    	<operationBinding operationId="searchAllTranspPlanesForCombosAv" operationType="fetch">
    		<selectClause>   			
    			t.transp_res_id,
    			t.transp_type_id,
    			t.name_descr 
			</selectClause> 
			<tableClause>transp_resource t</tableClause>	
			<whereClause>
				($defaultWhereClause) and t.transp_type_id = 1000005
			</whereClause>			
    	</operationBinding>
    	<operationBinding operationId="searchAllTranspPlanesForCombosRail" operationType="fetch">
    		<selectClause>   			
    			t.transp_res_id,
    			t.transp_type_id,
    			t.name_descr 
			</selectClause> 
			<tableClause>transp_resource t</tableClause>	
			<whereClause>
				($defaultWhereClause) and t.transp_type_id = 1000003
			</whereClause>			
    	</operationBinding>
    	<operationBinding operationId="searchAllTranspPlanesForCombosAvt" operationType="fetch">
    		<selectClause>   			
    			t.transp_res_id,
    			t.transp_type_id,
    			t.name_descr 
			</selectClause> 
			<tableClause>transp_resource t</tableClause>	
			<whereClause>
				($defaultWhereClause) and t.transp_type_id = 1000004
			</whereClause>			
    	</operationBinding>
    </operationBindings>	
</DataSource>