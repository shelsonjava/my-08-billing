<project name="CallCenterBK" basedir="." default="default">
 	<target name="default" depends="buildwar,deploy"></target>
 	<target name="buildwar">
		<war basedir="war" destfile="CallCenterBK.war" webxml="war/WEB-INF/web.xml">			
			<webinf dir="war/WEB-INF/">
				<include name="**/*.jar" />
			</webinf>
		</war>
	</target>
	<target name="deploy">
		<copy file="CallCenterBK.war" todir="." />
	</target>
</project>